{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ES2023"],
		"module": "NodeNext",
		"skipLibCheck": true,

		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": false,
		"isolatedModules": true,
		"moduleDetection": "force",

		"strict": true,
		"strictNullChecks": true
	},
	"files": []
}
